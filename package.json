{
  "name": "Petrus",
  "description": "A Piet compiler",
  "repository": {
    "type": "git",
    "url": "https://github.com/mizkichan/petrus"
  },
  "scripts": {
    "start": "parcel serve src/index.html",
    "prebuild": "npm run clean",
    "build": "parcel build -- --public-url . --no-source-maps src/index.html",
    "clean": "rimraf dist",
    "prepublish": "npm run build",
    "publish": "gh-pages -d dist"
  },
  "devDependencies": {
    "elm-hot": "^1.1.2",
    "gh-pages": "^2.1.1",
    "node-elm-compiler": "^5.0.4",
    "parcel-bundler": "^1.12.3",
    "rimraf": "^3.0.0",
    "sass": "^1.22.12"
  },
  "dependencies": {
    "bulma": "^0.7.5"
  },
  "private": true
}
